{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@content/*": ["src/content/*"],
      "@utils/*": ["src/utils/*"],
      "@schema/*": ["src/documentSchema/*"],
      "@type": ["src/types.ts"],
      "@db/*": ["db/*"],
      "@wc/*": ["src/webcomponents/*"]
    }
  }
}
