---
import { entityEnum } from "@utils/constants";

const { type, personCity, companyCity } = Astro.props;

let city;

switch (type) {
  case entityEnum[0]:
    city = personCity;
    break;
  case entityEnum[1]:
    city = companyCity;
    break;
  default:
    return Astro.redirect("/404");
}
---

{city}
