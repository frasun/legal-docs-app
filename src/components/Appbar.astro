---
import Menu from "@components/Menu.astro";
import Logo from "../images/logo.astro";
---

<header>
  <a href="/" id="logo" aria-label="Prawniczek">
    <Logo />
  </a>
  <Menu />
</header>

<style>
  header {
    @apply container;
    @apply flex items-center flex-wrap justify-between;
    @apply py-15;
    @apply sticky top-0;
    @apply bg-gray;
    @apply z-40;
  }
</style>
